{
  "name": "@ibm-watson/assistant-simple",
  "description": "A simple Node.js based web app which shows how to use the Watson Assistant API to recognize user intents.",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watson-developer-cloud/assistant-simple.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.2",
    "cross-fetch": "^3.1.3-alpha.6",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "ibm-watson": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "casperjs": "^1.1.4",
    "codecov": "^3.6.1",
    "eslint": "^7.11.0",
    "nodemon": "^2.0.7",
    "phantomjs-prebuilt": "^2.1.16",
    "supertest": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/watson-developer-cloud/assistant-simple/issues"
  },
  "homepage": "https://github.com/watson-developer-cloud/assistant-simple#readme",
  "directories": {
    "test": "test"
  },
  "author": ""
}
